{
    "version": 1,
    "request": {
        "url": "https://dblp.org/search/publ/api",
        "method": "GET",
        "params": {
            "q": true,
            "h": false,
            "f": false,
            "author": {
                "template": "author:{{first_name}}_{{last_name}}:",
                "required": false,
                "fromKey": ["first_name", "last_name"],
                "removeIfEmpty": true,
                "toKey": "q"
            }
        },
        "pagination": {
            "type": "offset",
            "offsetKey": "f",
            "limitKey": "h",
            "maxCount": 1000
        },
        "search": {
            "key": "q"
        }
    },
    "response": {
        "ctype": "application/xml",
        "tablePath": "/result/hits/*",
        "schema": {
            "authors": {
                "target": "info/authors/*/text()",
                "type": "object"
            },
            "title": {
                "target": "info/title/text()",
                "type": "string"
            },
            "venue": {
                "target": "info/venue/text()",
                "type": "object"
            },
            "pages": {
                "target": "info/pages/text()",
                "type": "string"
            },
            "publish year": {
                "target": "info/year/text()",
                "type": "string"
            },
            "publication type": {
                "target": "info/type/text()",
                "type": "string"
            },
            "publication url": {
                "target": "info/url/text()",
                "type": "string"
            }
        },
        "orient": "records"
    }
}
